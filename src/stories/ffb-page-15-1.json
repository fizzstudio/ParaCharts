{
  "datasets": [
    {
      "type": "pie",
      "title": "Your Board",
      "facets": {
        "x": {
          "label": "Satisfaction",
          "variableType": "independent",
          "measure": "nomimal",
          "datatype": "string"
        },
        "y": {
          "label": "Percent",
          "variableType": "dependent",
          "measure": "ratio",
          "datatype": "number"
        }
      },
      "series": [
        {
          "key": "Your Board",
          "theme": {
            "baseQuantity": "satisfaction",
            "baseKind": "dimensioned"
          },
          "records": [
            {
              "x": "Neutral",
              "y": "9"
            },
            {
              "x": "Satisfied",
              "y": "41"
            },
            {
              "x": "Very Satisfied",
              "y": "50"
            }
          ]
        }
      ],
      "data": {
        "source": "inline"
      },
      "settings": {
        "color.colorPalette": "ffb",
        "color.colorMap": "yellow,blue,indigo,light gray",
        "chart.orientation": "north",
        "chart.size.width": 360,
        "chart.title.align": "start",
        "axis.x.tick.tickLabel.angle": 0,
        "axis.y.tick.step": 1,
        "legend.isDrawLegend": false,
        "sonification.isEnabled": true
      }
    }
  ]
}
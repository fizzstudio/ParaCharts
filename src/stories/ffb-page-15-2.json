{
  "datasets": [
    {
      "type": "pie",
      "title": "Your Board",
      "facets": {
        "x": {
          "label": "Rewarding",
          "variableType": "independent",
          "measure": "nomimal",
          "datatype": "string",
          "displayType": {
            "type": "axis",
            "orientation": "horizontal"
          }
        },
        "y": {
          "label": "Percent",
          "variableType": "dependent",
          "measure": "ratio",
          "datatype": "number",
          "displayType": {
            "type": "axis",
            "orientation": "vertical"
          }
        }
      },
      "series": [
        {
          "key": "Your Board",
          "theme": {
            "baseQuantity": "satisfaction",
            "baseKind": "dimensioned"
          },
          "records": [
            {
              "x": "Very Unrewarding",
              "y": "3"
            },
            {
              "x": "Neutral",
              "y": "3"
            },
            {
              "x": "Somewhat Rewarding",
              "y": "10"
            },
            {
              "x": "Very Rewarding",
              "y": "80"
            },
            {
              "x": "No Opinion",
              "y": "3"
            }
          ]
        }
      ],
      "data": {
        "source": "inline"
      },
      "settings": {
        "color.colorPalette": "ffb",
        "color.colorMap": "red,yellow,blue,indigo,light gray",
        "chart.orientation": "north",
        "chart.size.width": 360,
        "chart.title.align": "start",
        "axis.x.tick.tickLabel.angle": 0,
        "axis.y.tick.step": 1,
        "legend.isDrawLegend": false,
        "sonification.isSoniEnabled": true
      }
    }
  ]
